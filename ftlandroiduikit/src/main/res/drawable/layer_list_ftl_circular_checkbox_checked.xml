<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/bg_checked">
        <shape android:shape="oval">
            <solid android:color="@color/IconBlueLight" />
            <stroke
                android:width="2dp"
                android:color="@color/IconBlueLight" />
            <size
                android:width="24dp"
                android:height="24dp" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:drawable="@drawable/ic_check_24"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp" />
</layer-list>

